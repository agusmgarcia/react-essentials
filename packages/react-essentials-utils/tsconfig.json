{
  "extends": "@agusmgarcia/react-essentials-commands/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "paths": {
      "#src/*": ["src/*"]
    },
    "rootDir": "./src"
  },
  "exclude": [".next", "bin", "dist", "node_modules"],
  "include": ["**/*.ts", "**/*.tsx"]
}
