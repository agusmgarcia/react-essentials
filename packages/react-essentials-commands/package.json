{
  "name": "@agusmgarcia/react-essentials-commands",
  "core": "lib",
  "version": "0.12.0",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/_out/*.d.ts",
      "default": "./dist/_out/*.js"
    },
    "./tsconfig.json": {
      "default": "./dist/_out/tsconfig.base.json"
    }
  },
  "author": "Agus Garcia",
  "description": "Set of opinionated commands for NextJS applications, libraries and Azure functions",
  "bin": {
    "react-essentials-commands-build": "bin/build.js",
    "react-essentials-commands-check": "bin/check.js",
    "react-essentials-commands-deploy": "bin/deploy.js",
    "react-essentials-commands-format": "bin/format.js",
    "react-essentials-commands-regenerate": "bin/regenerate.js",
    "react-essentials-commands-start": "bin/start.js",
    "react-essentials-commands-test": "bin/test.js"
  },
  "scripts": {
    "build": "tsx src/_bin/build.ts",
    "check": "tsx src/_bin/check.ts",
    "deploy": "tsx src/_bin/deploy.ts --path=../../.github/workflows",
    "format": "tsx src/_bin/format.ts",
    "postbuild": "chmod +x bin/*.js",
    "prebuild": "npm i --ignore-scripts --no-audit --no-fund",
    "regenerate": "tsx src/_bin/regenerate.ts --path=../../.github/workflows",
    "start": "tsx src/_bin/start.ts",
    "test": "tsx src/_bin/test.ts"
  },
  "peerDependencies": {
    "@next/env": "^15.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0 || ^19.0.0",
    "concurrently": "^9.0.0",
    "css-loader": "^7.0.0",
    "del-cli": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-sort": "^4.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "next": "^15.0.0",
    "postcss-loader": "^8.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "semver": "^7.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "ts-loader": "^9.0.0",
    "ts-transform-paths": "^3.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^6.0.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@next/env": "^15.5.2",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "~18.18.0",
    "@types/react": "^19.1.12",
    "@types/semver": "^7.7.0",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "del-cli": "^6.0.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sort": "^4.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "next": "^15.5.2",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semver": "^7.7.2",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.12",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-transform-paths": "^3.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "yaml": "^2.8.1"
  },
  "files": [
    "bin",
    "dist"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agusmgarcia/react-essentials.git"
  }
}
